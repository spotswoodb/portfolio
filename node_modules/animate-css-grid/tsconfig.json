    "target": "es5",
    "lib": ["es2015", "dom"],
    "moduleResolution": "node",
    "allowJs":false,
    "outDir": "dist",
    "emitDeclarationOnly": true,
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "noEmitOnError": true,
  },
  "include": ["src"]
}
